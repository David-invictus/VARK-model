###  http://example.com#DominantStyle
:DominantStyle rdf:type owl:Class ;
               rdfs:subClassOf :Classify ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :preferAuralAuditive ;
                                 owl:minQualifiedCardinality "10"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:integer
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :preferKinesthetic ;
                                 owl:minQualifiedCardinality "6"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:integer
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :preferReadWrite ;
                                 owl:minQualifiedCardinality "4"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:integer
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :preferVisual ;
                                 owl:minQualifiedCardinality "7"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:integer
                               ] .


