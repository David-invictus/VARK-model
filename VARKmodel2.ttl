###  http://example.com#Classify
:Classify rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( gufo:IntrinsicMode
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty gufo:inheresIn ;
                                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onClass :Learner
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf gufo:IntrinsicMode .


